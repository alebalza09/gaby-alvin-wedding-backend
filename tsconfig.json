{
    "display": "Node 19",
    "version": "19.3.0",
    "compilerOptions": {
        "lib": ["es2023"],
        "module": "ESNext",
        "target": "ES2022",
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "Bundler",
        "outDir": "dist",
        "rootDir": "."
    },
    "include": ["src"]
}